<%= simple_form_for @restaurant, url: admin_restaurant_path, method: :patch do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, as: :select, collection: Restaurant::CATEGORIES, include_blank:  "Select a category" %>
  <%= f.submit %>
<% end %>

